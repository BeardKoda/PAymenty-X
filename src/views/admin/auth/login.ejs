<body>

    <div id="preloader" class="d-none">
        <div class="loader d-none">
            <div class="loader__bar"></div>
            <div class="loader__bar"></div>
            <div class="loader__bar"></div>
            <div class="loader__bar"></div>
            <div class="loader__bar"></div>
            <div class="loader__ball"></div>
        </div>
    </div>

    <div class="login-section h-100">

            <div class="row h-100 mx-auto justify-content-center align-items-center text-center">
                <div class="col-lg-12">
                    <h2 class="page-title"></h2>
                </div>

                <div class="col-md-6 col-lg-5 col-xl-4">
                    <div class="mx-auto card login-card">
                        <div class="card-body">
                            <h2 class="card-title">Administrator Login</h2>
                            <form method="POST" action="login">
                                <div class="form-group">
                                    <input type="email" name="email" class="form-control m-b-30"  aria-describedby="emailHelp" placeholder="Email">
                                </div>
                                <div class="form-group">
                                    <input type="password" name="password" class="form-control m-b-30" placeholder="Password">
                                </div>
                                <div class="form-group">
                                    <div class="custom-control custom-checkbox mr-3">
                                        <input type="checkbox" class="custom-control-input" id="customCheck1">
                                        <label class="custom-control-label" for="customCheck1">Remeber Me</label>
                                    </div>
                                </div>
                                <button type="submit" class="btn btn-block m-b-30 submit">Login</button>
                            </form>
                        </div>
                    </div>
                </div>

                <div class="col-lg-12">
                    <p class="copyright">&copy; Copyright 2018 by BrindoCorp. All Rights Reserved.</p>
                </div>
            </div>

            <!-- <div class="row">
                <div class="col-lg-12">
